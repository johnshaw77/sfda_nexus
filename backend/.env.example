# 圖表檢測功能開關
# 設為 'false' 可完全禁用圖表檢測功能
ENABLE_CHART_DETECTION=true

# 圖表檢測嚴格模式
# 設為 'true' 啟用超嚴格模式，只有明確說出圖表關鍵詞才會觸發
CHART_DETECTION_STRICT_MODE=false

# ===== AI 二次調用模型配置 =====
# 🤖 提示詞優化專用模型配置
# 用於 handleOptimizePrompt 功能，使用輕量級模型進行快速優化
PROMPT_OPTIMIZATION_MODEL_ID=46
PROMPT_OPTIMIZATION_MODEL_NAME=qwen2.5:1.5b

# 🧠 AI 總結專用模型配置  
# 用於 MCP 工具調用後的智能總結功能
AI_SUMMARY_MODEL_ID=47
AI_SUMMARY_MODEL_NAME=qwen2.5:14b

# ... 其他現有配置 